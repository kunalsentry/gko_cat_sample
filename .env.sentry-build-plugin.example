# Copy this file to .env.sentry-build-plugin and fill in your actual values
# This file is used during build time to upload source maps to Sentry

# Get your auth token from https://sentry.io/settings/account/api/auth-tokens/
SENTRY_AUTH_TOKEN=your-auth-token-here

# Your Sentry organization slug
SENTRY_ORG=your-org-slug

# Your Sentry project slug
SENTRY_PROJECT=your-project-slug
